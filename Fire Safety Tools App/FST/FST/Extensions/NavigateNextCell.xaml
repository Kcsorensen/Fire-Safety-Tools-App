<?xml version="1.0" encoding="utf-8" ?>
<ViewCell xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FST.Extensions.NavigateNextCell">

    <StackLayout Orientation="Horizontal" Padding="15, 0, 5, 0">
        <Label Text="{Binding Label}" VerticalOptions="Center"/>
        <Image Source="next.png" VerticalOptions="Center" HorizontalOptions="EndAndExpand"/>
    </StackLayout>

</ViewCell>