<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FST.App">
	<Application.Resources>

        <ResourceDictionary>
            <Style x:Key="defaultParentPageStyle" TargetType="Layout">
                <Setter Property="Margin" Value="15, 15, 10, 15"/>
            </Style>
            <Style x:Key="defaultCellStyle" TargetType="Layout">
                <Setter Property="Padding" Value="15, 8, 10, 8"/>
            </Style>
        </ResourceDictionary>

	</Application.Resources>
</Application>